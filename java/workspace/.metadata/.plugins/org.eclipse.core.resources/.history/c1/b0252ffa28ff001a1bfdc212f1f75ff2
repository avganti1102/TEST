package Lesson7;

public class main {
	public static void main(String[] args) {
		/*
		 * -----final------
		 * - Là một biến không thể thay đổi giá trị(hằng số)
		 */
		Person p1 = new Person();
		System.out.println(p1.getIdPerson());
		Person p2 = new Person();
		System.out.println(p2.getIdPerson());
		System.out.println(p1.getNumber());
		System.out.println(p2.getNumber());
		System.out.println(Person.getIdPerson());
	}
}
