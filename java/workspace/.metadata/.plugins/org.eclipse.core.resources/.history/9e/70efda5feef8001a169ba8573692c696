package Assignment3;

import java.util.Random;

public class Ex1 {
	public static void main(String[] args){
		
//Q1
		float a = 5240.5f;
		float b = 10970.055f;
		
		int c = (int)a;
		int d = (int)b;
		System.out.println("Lương Account 1 làm tròn: " + c + "$");
		System.out.println("Lương Account 2 làm tròn: " + d+ "$");
		
//Q2
		Random rd = new Random();
		int random = rd.nextInt(10000);
		String str = String.valueOf(random);
		System.out.println(str);
		System.out.println(str.codePointCount(0, 3));
	}
}
