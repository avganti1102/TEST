package Lesson10;

import java.util.ArrayList;
import java.util.Queue;

public class Map_Collection {
	// Collection: Tập hợp các phần tử ( tập hơp từng phần tử)
	// Map : Tập hợp các phần tử ( theo các cặp giá trị key-value)
	/*
	 * -------I-Colection-----
	 * -- I-List
	 * 		- Danh sách bất kì, Các phần tử có cùng kiểu dữ liệu
	 * 		- Cho phép lặp
	 * -- I-Queue
	 * 		- Cho phép lặp
	 * -- I-Set
	 * 		- K cho phép lặp
	 */
	
	/*
	 * ---------List--------
	 * - ArrayList(class)
	 * 	+ get(index) : nhanh
	 * 	+ add() : O(n) - khi phải mở rộng kích thước mảng 
	 * 	+ insert : chậm khi phải mở rộng kích thước lưu trữ mảng O(n)
	 * 	+ remove(): độ phức tạp (n) = kích thước mảng --> chậm
	 * - LinkedList(class)
	 * 	+ get : O(n)
	 * 	+ add : O(1)
	 * 	+ insert : O(n)
	 *  + remove : O(n)
	 *  + 
	 * - Vector --Stack : *Đồng bộ hóa
	 */
	
