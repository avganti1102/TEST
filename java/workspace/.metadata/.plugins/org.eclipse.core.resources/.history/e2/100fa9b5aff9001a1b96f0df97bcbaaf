package Lesson4;

import java.util.Scanner;

public class test {
	static Scanner sc = new Scanner(System.in);
	static String str = "";
	
	
	public static void main(String[] args){
		String reverse2 = "";
		System.out.println("Nhập chuỗi");
		str = sc.nextLine();
		str.trim();
		for (int i = str.length() - 1; i >= 0; i--) {
			reverse2 += str.charAt(i);
		}
		word = reverse2.split(" ");
		for(String a : word){
			System.out.print(a + " ");
	}
}
