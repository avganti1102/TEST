package Ex5;

public class KySu extends CanBo{
	private String nganhDaoTao;

	public String getNganhDaoTao() {
		return nganhDaoTao;
	}
	
	@Override
	public void input() {
		Scanner sc = new Scanner(System.in);
		super.input();
		System.out.println("Nhập ngành đào tạo: ");
		nganhDaoTao = sc.nextLine();
	}
}
