package Lesson4.Mobile;

public class Mobile {
	protected String OS;
	private String name;
	public Mobile(String name){
		this.name = name;
	}
	
	public void call(){
		System.out.println("Calling!!");
	}
	
	public void call(String phoneNumber){
		System.out.println("Calling to " + phoneNumber);
	}
}
