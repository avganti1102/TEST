package Lesson10;

public class Person<T> implements Comparable<Person>{
	private String name;
	private int point;
	private T id;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public Person(String name,int point) {
		 this.name = name;
		 this.point = point;
	 }
	
	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public int getPoint() {
		return point;
	}


	public void setPoint(int point) {
		this.point = point;
	}

	//Tiêu chí sắp xếp
	@Override
	public int compareTo(Person o) {
		if(this.getName().compareTo(o.getName()) > 0 )
			return 1;
		else if(this.getName().compareTo(o.getName()) < 0 )
			return -1;
		else
			return 0;
	}
}
