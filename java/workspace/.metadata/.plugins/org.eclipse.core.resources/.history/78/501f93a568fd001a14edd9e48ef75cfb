package Ex2.Question1;

public class Student implements IStudent {
	private short id;
	private String name;
	private int group;
	
	public Student(short id,String name,int group)
	
	@Override
	public void diemDanh() {
		System.out.println(name + " điểm danh");
	}
	@Override
	public void hocBai() {
		System.out.println(name + " học bài");
	}
	@Override
	public void donVS() {
		System.out.println(name + " dọn vệ sinh");
	}
	
	
}
